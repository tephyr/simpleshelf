{
    "libraryModules": [
        "jquery",
        "underscore",
        "underscore.string",
        "handlebars",
        "backbone",
        "md5",
        "tether",
        "selectize"
    ],
    "testLibraryModules": [
        "mocha",
        "chai"
    ],
    "externalUIJSDev": [
        "node_modules/bootstrap/dist/js/bootstrap.js",
        "node_modules/bootstrap-datepicker/dist/js/bootstrap-datepicker.js"
    ],
    "externalUICSSDev": [
        "node_modules/bootstrap-datepicker/dist/css/bootstrap-datepicker.css*",
        "node_modules/selectize/dist/css/selectize.css"
    ],
    "externalUICSSTest": [
        "node_modules/mocha/mocha.css"
    ],
    "clientSource": "./app",
    "ddocSource": "./app-ddoc/",
    "testSource": "./app/code/test/",
    "destination": "http://127.0.0.1:5984/simpleshelf",
    "outputDDoc": "./output-ddoc",
    "outputTest": "./output-test",
    "_docs": {
        "configurationDefault": "./config/default-docs/simpleshelf-configuration-default.json",
        "i18nDefault": "./config/default-docs/simpleshelf-i18n-default.json"
    }
}
